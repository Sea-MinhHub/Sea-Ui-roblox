-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- TAB 3: PLAYER
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local PlayerTab = SeaMinhUI:AddTab(Window, {
    Title = "ğŸ‘¤ Player",
    Icon = "user"
})

-- Walk Speed
local SpeedSlider = SeaMinhUI:AddSlider(PlayerTab, {
    Title = "Walk Speed",
    Description = "Change your walking speed",
    Min = 16,
    Max = 200,
    Default = 16,
    Rounding = 0,
    Callback = function(value)
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = value
        end
    end
})

-- Jump Power
SeaMinhUI:AddSlider(PlayerTab, {
    Title = "Jump Power",
    Description = "Change your jump height",
    Min = 50,
    Max = 200,
    Default = 50,
    Rounding = 0,
    Callback = function(value)
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.JumpPower = value
        end
    end
})

-- God Mode
SeaMinhUI:AddToggle(PlayerTab, {
    Title = "God Mode",
    Description = "Take no damage",
    Default = false,
    Callback = function(value)
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            if value then
                player.Character.Humanoid.MaxHealth = math.huge
                player.Character.Humanoid.Health = math.huge
            else
                player.Character.Humanoid.MaxHealth = 100
                player.Character.Humanoid.Health = 100
            end
        end
    end
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- TAB 4: TELEPORT
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local TeleportTab = SeaMinhUI:AddTab(Window, {
    Title = "ğŸš€ Teleport",
    Icon = "rocket"
})

-- Teleport to locations
local locations = {
    ["Spawn"] = Vector3.new(0, 10, 0),
    ["Shop"] = Vector3.new(100, 10, 100),
    ["Boss Area"] = Vector3.new(-200, 50, -200),
    ["Safe Zone"] = Vector3.new(500, 20, 500)
}

for name, position in pairs(locations) do
    SeaMinhUI:AddButton(TeleportTab, {
        Title = "Teleport to " .. name,
        Description = "Click to teleport",
        Callback = function()
            local player = game.Players.LocalPlayer
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                player.Character.HumanoidRootPart.CFrame = CFrame.new(position)
                print("âœˆï¸ Teleported to", name)
            end
        end
    })
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- TAB 5: SETTINGS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local SettingsTab = SeaMinhUI:AddTab(Window, {
    Title = "âš™ï¸ Settings",
    Icon = "settings"
})

-- Theme Selection
SeaMinhUI:AddDropdown(SettingsTab, {
    Title = "UI Theme",
    Description = "Change the UI theme",
    Values = {"Dark", "Light", "Aqua", "Amethyst", "Rose", "Darker"},
    Default = "Aqua",
    Callback = function(value)
        SeaMinhUI:SetTheme(value)
        print("ğŸ¨ Theme changed to:", value)
    end
})

-- Discord Username
SeaMinhUI:AddInput(SettingsTab, {
    Title = "Discord Username",
    Description = "Enter your Discord username",
    Default = "",
    Placeholder = "username#1234",
    Callback = function(text)
        print("Discord:", text)
    end
})

-- Save Config
SeaMinhUI:AddButton(SettingsTab, {
    Title = "Save Configuration",
    Description = "Save current settings",
    Callback = function()
        print("ğŸ’¾ Configuration saved!")
        SeaMinhUI:Notify({
            Title = "Sea Minh UI",
            Content = "Configuration saved!",
            Duration = 3
        })
    end
})

-- Destroy UI
SeaMinhUI:AddButton(SettingsTab, {
    Title = "Unload UI",
    Description = "Destroy the UI",
    Callback = function()
        SeaMinhUI:Destroy()
        print("ğŸ—‘ï¸ UI destroyed!")
    end
})

