-- ============================================================
-- 1. T·∫†O WINDOW C∆† B·∫¢N
-- ============================================================
--[[ COPY ƒêO·∫†N N√ÄY ƒê·ªÇ T·∫†O WINDOW ]]--
local UI = SeaMinhUI
local window = UI:CreateWindow({
    Title = "My Script",
    Size = UDim2.fromOffset(600, 500)
})

-- ============================================================
-- 2. T·∫†O WINDOW V·ªöI ICON V√Ä SUBTITLE
-- ============================================================
--[[ WINDOW ƒê·∫¶Y ƒê·ª¶ T√çNH NƒÇNG ]]--
local window = UI:CreateWindow({
    Title = "My Script Hub",
    SubTitle = "Version 1.0.0",
    Icon = "home", -- home, settings, user, star
    Size = UDim2.fromOffset(650, 550),
    MinimizeKey = Enum.KeyCode.RightControl, -- Ph√≠m ·∫©n/hi·ªán UI
    Center = true -- true = gi·ªØa m√†n h√¨nh, false = g√≥c
})

-- ============================================================
-- 3. T·∫†O TAB C√ì ICON
-- ============================================================
--[[ TAB C∆† B·∫¢N ]]--
local tab1 = window:AddTab({
    Title = "Home"
})

--[[ TAB C√ì ICON ]]--
local tab2 = window:AddTab({
    Title = "Settings",
    Icon = "settings" -- home, settings, user, star, info, bell
})

-- ============================================================
-- 4. T·∫†O SECTION (KHUNG NH√ìM)
-- ============================================================
--[[ SECTION ƒê·ªÇ NH√ìM C√ÅC N√öT L·∫†I ]]--
local section1 = tab1:AddSection("My Section Title")

-- ============================================================
-- 5. N√öT BUTTON C∆† B·∫¢N
-- ============================================================
--[[ BUTTON ƒê·ªñN GI·∫¢N ]]--
tab1:AddButton({
    Title = "Click Me",
    Callback = function()
        print("Button clicked!")
    end
})

--[[ BUTTON C√ì ICON ]]--
tab1:AddButton({
    Title = "Save Settings",
    Icon = "check", -- check, x, home, settings, star
    Callback = function()
        print("Settings saved!")
    end
})

-- ============================================================
-- 6. TOGGLE (C√îNG T·∫ÆC B·∫¨T/T·∫ÆT)
-- ============================================================
--[[ TOGGLE C∆† B·∫¢N ]]--
local myToggle = tab1:AddToggle({
    Title = "Enable Feature",
    Default = false, -- Tr·∫°ng th√°i ban ƒë·∫ßu
    Callback = function(value)
        print("Toggle:", value) -- true ho·∫∑c false
    end
})

--[[ S·ª¨ D·ª§NG TOGGLE ]]--
-- ƒê·ªçc gi√° tr·ªã
local isEnabled = myToggle:GetValue()

-- Thay ƒë·ªïi gi√° tr·ªã
myToggle:SetValue(true) -- B·∫≠t
myToggle:SetValue(false) -- T·∫Øt

-- ============================================================
-- 7. SLIDER (THANH K√âO)
-- ============================================================
--[[ SLIDER S·ªê NGUY√äN ]]--
local speedSlider = tab1:AddSlider({
    Title = "Walk Speed",
    Min = 16,
    Max = 100,
    Default = 16,
    Rounding = 1, -- L√†m tr√≤n ƒë·∫øn s·ªë nguy√™n
    Callback = function(value)
        print("Speed:", value)
    end
})

--[[ SLIDER S·ªê TH·∫¨P PH√ÇN ]]--
local volumeSlider = tab1:AddSlider({
    Title = "Volume",
    Min = 0,
    Max = 1,
    Default = 0.5,
    Rounding = 0.1, -- L√†m tr√≤n 1 ch·ªØ s·ªë
    Callback = function(value)
        print("Volume:", value)
    end
})

--[[ S·ª¨ D·ª§NG SLIDER ]]--
-- ƒê·ªçc gi√° tr·ªã
local speed = speedSlider:GetValue()

-- Thay ƒë·ªïi gi√° tr·ªã
speedSlider:SetValue(50)

-- ============================================================
-- 8. INPUT (NH·∫¨P TEXT)
-- ============================================================
--[[ INPUT C∆† B·∫¢N ]]--
local nameInput = tab1:AddInput({
    Title = "Your Name",
    Placeholder = "Enter your name...",
    Default = "Player",
    Callback = function(value)
        print("Name:", value)
    end
})

--[[ S·ª¨ D·ª§NG INPUT ]]--
-- ƒê·ªçc text
local playerName = nameInput:GetValue()

-- Thay ƒë·ªïi text
nameInput:SetValue("NewName")

-- ============================================================
-- 9. DROPDOWN (MENU CH·ªåN)
-- ============================================================
--[[ DROPDOWN C∆† B·∫¢N ]]--
local modeDropdown = tab1:AddDropdown({
    Title = "Select Mode",
    Values = {"Easy", "Normal", "Hard", "Extreme"},
    Default = "Normal",
    Callback = function(value)
        print("Selected:", value)
    end
})

--[[ S·ª¨ D·ª§NG DROPDOWN ]]--
-- ƒê·ªçc gi√° tr·ªã ƒë√£ ch·ªçn
local selectedMode = modeDropdown:GetValue()

-- Thay ƒë·ªïi l·ª±a ch·ªçn
modeDropdown:SetValue("Hard")

-- ============================================================
-- 10. COLOR PICKER (CH·ªåN M√ÄU)
-- ============================================================
--[[ COLOR PICKER ]]--
local colorPicker = tab1:AddColorPicker({
    Title = "Primary Color",
    Default = Color3.fromRGB(255, 0, 0), -- M√†u ƒë·ªè
    Callback = function(color)
        print("Color:", color)
    end
})

--[[ S·ª¨ D·ª§NG COLOR PICKER ]]--
-- ƒê·ªçc m√†u
local currentColor = colorPicker:GetColor()

-- Thay ƒë·ªïi m√†u
colorPicker:SetColor(Color3.fromRGB(0, 255, 0)) -- M√†u xanh l√°

-- ============================================================
-- 11. KEYBIND (CH·ªåN PH√çM)
-- ============================================================
--[[ KEYBIND C∆† B·∫¢N ]]--
local flyKeybind = tab1:AddKeybind({
    Title = "Fly Mode",
    Default = Enum.KeyCode.F,
    Callback = function(key)
        print("Key changed to:", key.Name)
    end,
    OnPress = function()
        print("Fly key pressed!")
        -- Code khi nh·∫•n ph√≠m
    end
})

--[[ S·ª¨ D·ª§NG KEYBIND ]]--
-- ƒê·ªçc ph√≠m hi·ªán t·∫°i
local currentKey = flyKeybind:GetKey()

-- Thay ƒë·ªïi ph√≠m
flyKeybind:SetKey(Enum.KeyCode.G)

-- ============================================================
-- 12. LABEL (HI·ªÇN TH·ªä TEXT)
-- ============================================================
--[[ LABEL C∆† B·∫¢N ]]--
tab1:AddLabel({
    Text = "This is a normal label"
})

--[[ LABEL IN ƒê·∫¨M ]]--
tab1:AddLabel({
    Text = "This is bold text",
    Bold = true
})

--[[ LABEL C·ª† CH·ªÆ L·ªöN ]]--
tab1:AddLabel({
    Text = "Big title text",
    Bold = true,
    Size = 16
})

--[[ C·∫¨P NH·∫¨T LABEL ]]--
local myLabel = tab1:AddLabel({
    Text = "Initial text"
})
myLabel:SetText("Updated text")

-- ============================================================
-- 13. DIVIDER (ƒê∆Ø·ªúNG PH√ÇN C√ÅCH)
-- ============================================================
--[[ ƒê∆Ø·ªúNG PH√ÇN C√ÅCH ]]--
tab1:AddDivider()

-- ============================================================
-- 14. TH√îNG B√ÅO - SUCCESS (TH√ÄNH C√îNG)
-- ============================================================
--[[ TH√îNG B√ÅO TH√ÄNH C√îNG - M√ÄU XANH L√Å ]]--
UI:Notify({
    Title = "Success!",
    Content = "Operation completed successfully",
    Duration = 3,
    Type = "Success"
})

-- ============================================================
-- 15. TH√îNG B√ÅO - INFO (TH√îNG TIN)
-- ============================================================
--[[ TH√îNG B√ÅO TH√îNG TIN - M√ÄU XANH D∆Ø∆†NG ]]--
UI:Notify({
    Title = "Information",
    Content = "This is an informational message",
    Duration = 3,
    Type = "Info"
})

-- ============================================================
-- 16. TH√îNG B√ÅO - WARNING (C·∫¢NH B√ÅO)
-- ============================================================
--[[ TH√îNG B√ÅO C·∫¢NH B√ÅO - M√ÄU V√ÄNG ]]--
UI:Notify({
    Title = "Warning!",
    Content = "Please be careful with this action",
    Duration = 4,
    Type = "Warning"
})

-- ============================================================
-- 17. TH√îNG B√ÅO - ERROR (L·ªñI)
-- ============================================================
--[[ TH√îNG B√ÅO L·ªñI - M√ÄU ƒê·ªé ]]--
UI:Notify({
    Title = "Error!",
    Content = "Something went wrong",
    Duration = 5,
    Type = "Error"
})

-- ============================================================
-- 18. TH√îNG B√ÅO C√ì ICON
-- ============================================================
--[[ TH√îNG B√ÅO V·ªöI ICON T√ôY CH·ªàNH ]]--
UI:Notify({
    Title = "Custom Notification",
    Content = "With custom icon",
    Duration = 3,
    Type = "Success",
    Icon = "star" -- bell, check, x, star, home, settings
})

-- ============================================================
-- 19. ƒêI·ªÄU KHI·ªÇN WINDOW
-- ============================================================
--[[ ·∫®N WINDOW ]]--
window:Minimize()

--[[ HI·ªÜN WINDOW ]]--
window:Restore()

--[[ B·∫¨T/T·∫ÆT WINDOW ]]--
window:Toggle()

--[[ ƒê√ìNG/X√ìA WINDOW ]]--
window:Destroy()

-- ============================================================
-- 20. L∆ØU V√Ä T·∫¢I SETTINGS
-- ============================================================
--[[ L∆ØU TH·ª¶ C√îNG ]]--
SaveManager:Save()

--[[ ƒê·ªåC D·ªÆ LI·ªÜU ]]--
local savedValue = SaveManager:Get("myKey", "defaultValue")

--[[ GHI D·ªÆ LI·ªÜU ]]--
SaveManager:Set("myKey", "myValue")

--[[ X√ìA D·ªÆ LI·ªÜU ]]--
SaveManager.Data = {}
SaveManager:Save()

-- ============================================================
-- 21. V√ç D·ª§ HO√ÄN CH·ªàNH - TAB V·ªöI NHI·ªÄU ELEMENTS
-- ============================================================
--[[ V√ç D·ª§ ƒê·∫¶Y ƒê·ª¶ ]]--
local window = UI:CreateWindow({
    Title = "My Hub",
    Icon = "home",
    Size = UDim2.fromOffset(600, 500)
})

local mainTab = window:AddTab({
    Title = "Main",
    Icon = "home"
})

local section1 = mainTab:AddSection("Player Settings")

section1:AddToggle({
    Title = "Auto Farm",
    Default = false,
    Callback = function(value)
        if value then
            UI:Notify({
                Title = "Enabled",
                Content = "Auto Farm activated!",
                Type = "Success"
            })
        else
            UI:Notify({
                Title = "Disabled",
                Content = "Auto Farm deactivated",
                Type = "Info"
            })
        end
    end
})

section1:AddSlider({
    Title = "Farm Speed",
    Min = 1,
    Max = 10,
    Default = 5,
    Callback = function(value)
        print("Farm speed:", value)
    end
})

local section2 = mainTab:AddSection("Actions")

section2:AddButton({
    Title = "Teleport to Boss",
    Icon = "star",
    Callback = function()
        UI:Notify({
            Title = "Teleporting...",
            Content = "Moving to boss location",
            Type = "Info",
            Duration = 2
        })
    end
})

section2:AddButton({
    Title = "Reset Character",
    Icon = "x",
    Callback = function()
        UI:Notify({
            Title = "Reset!",
            Content = "Character has been reset",
            Type = "Warning",
            Duration = 2
        })
    end
})

