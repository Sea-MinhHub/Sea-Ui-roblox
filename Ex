-- ============================================================
-- BÆ¯á»šC 1: LOAD LIBRARY
-- ============================================================

local SeaMinhUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Sea-MinhHub/Sea-Ui-roblox/main/Main.lua"))()

-- ============================================================
-- BÆ¯á»šC 2: Táº O WINDOW (Cá»¬A Sá»” CHÃNH)
-- ============================================================

local Window = SeaMinhUI:CreateWindow({
    Title = "Sea Minh Hub",              -- TiÃªu Ä‘á» (Báº®T BUá»˜C)
    SubTitle = "Premium Script v2.0",    -- Phá»¥ Ä‘á» (TÃ™Y CHá»ŒN)
    Size = UDim2.fromOffset(580, 460),   -- KÃ­ch thÆ°á»›c (Máº¶C Äá»ŠNH: 580x460)
    TabWidth = 150,                      -- Äá»™ rá»™ng tab (Máº¶C Äá»ŠNH: 150)
    Theme = "Dark",                      -- Theme: Dark, Light, Aqua, etc (Máº¶C Äá»ŠNH: Dark)
    Acrylic = true,                      -- Hiá»‡u á»©ng ná»n má» (Máº¶C Äá»ŠNH: false)
    MinimizeKey = Enum.KeyCode.RightControl, -- PhÃ­m minimize (Máº¶C Äá»ŠNH: LeftControl)
    
    -- ICON (TÃ™Y CHá»ŒN)
    Icon = "crown",                      -- TÃªn icon hoáº·c Asset ID
    IconSize = 24                        -- KÃ­ch thÆ°á»›c icon (Máº¶C Äá»ŠNH: 20)
})

-- ============================================================
-- BÆ¯á»šC 3: Táº O TAB (TRANG)
-- ============================================================

local MainTab = SeaMinhUI:AddTab(Window, {
    Title = "Main",                      -- TiÃªu Ä‘á» tab (Báº®T BUá»˜C)
    Icon = "home"                        -- Icon tab (TÃ™Y CHá»ŒN)
})

local CombatTab = SeaMinhUI:AddTab(Window, {
    Title = "Combat",
    Icon = "sword"
})

local PlayerTab = SeaMinhUI:AddTab(Window, {
    Title = "Player",
    Icon = "user"
})

local SettingsTab = SeaMinhUI:AddTab(Window, {
    Title = "Settings",
    Icon = "settings"
})

-- ============================================================
-- BÆ¯á»šC 4: THÃŠM CÃC ELEMENTS
-- ============================================================

-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- 4.1: BUTTON (NÃšT Báº¤M)
-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

SeaMinhUI:AddButton(MainTab, {
    Title = "Auto Farm",                 -- TiÃªu Ä‘á» (Báº®T BUá»˜C)
    Description = "Farm coins automatically", -- MÃ´ táº£ (TÃ™Y CHá»ŒN)
    Callback = function()
        print("Auto Farm activated!")
        -- Code cá»§a báº¡n á»Ÿ Ä‘Ã¢y
    end
})

-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- 4.2: TOGGLE (CÃ”NG Táº®C Báº¬T/Táº®T)
-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

local AutoFarmToggle = SeaMinhUI:AddToggle(MainTab, {
    Title = "Auto Farm",                 -- TiÃªu Ä‘á» (Báº®T BUá»˜C)
    Description = "Enable auto farm",    -- MÃ´ táº£ (TÃ™Y CHá»ŒN)
    Default = false,                     -- GiÃ¡ trá»‹ máº·c Ä‘á»‹nh (Máº¶C Äá»ŠNH: false)
    Callback = function(value)
        print("Auto Farm:", value)
        
        if value then
            print("Auto Farm enabled!")
            -- Báº­t auto farm
        else
            print("Auto Farm disabled!")
            -- Táº¯t auto farm
        end
    end
})

-- Thay Ä‘á»•i giÃ¡ trá»‹ Toggle tá»« code:
AutoFarmToggle.SetValue(true)  -- Báº­t
AutoFarmToggle.SetValue(false) -- Táº¯t

-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- 4.3: SLIDER (THANH TRÆ¯á»¢T)
-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

local SpeedSlider = SeaMinhUI:AddSlider(PlayerTab, {
    Title = "Walk Speed",                -- TiÃªu Ä‘á» (Báº®T BUá»˜C)
    Description = "Adjust your speed",   -- MÃ´ táº£ (TÃ™Y CHá»ŒN)
    Min = 16,                           -- GiÃ¡ trá»‹ nhá» nháº¥t (Báº®T BUá»˜C)
    Max = 100,                          -- GiÃ¡ trá»‹ lá»›n nháº¥t (Báº®T BUá»˜C)
    Default = 16,                       -- GiÃ¡ trá»‹ máº·c Ä‘á»‹nh (Báº®T BUá»˜C)
    Rounding = 0,                       -- LÃ m trÃ²n (0 = sá»‘ nguyÃªn, 1 = 1 chá»¯ sá»‘ tháº­p phÃ¢n)
    Callback = function(value)
        print("Speed set to:", value)
        
        -- Set speed
        local player = game.Players.LocalPlayer
        if player.Character and player.Character:FindFirstChild("Humanoid") then
            player.Character.Humanoid.WalkSpeed = value
        end
    end
})

-- Thay Ä‘á»•i giÃ¡ trá»‹ Slider tá»« code:
SpeedSlider.SetValue(50)

-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- 4.4: INPUT (Ã” NHáº¬P LIá»†U)
-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

local UsernameInput = SeaMinhUI:AddInput(SettingsTab, {
    Title = "Target Username",           -- TiÃªu Ä‘á» (Báº®T BUá»˜C)
    Description = "Enter target name",   -- MÃ´ táº£ (TÃ™Y CHá»ŒN)
    Default = "",                        -- GiÃ¡ trá»‹ máº·c Ä‘á»‹nh (Máº¶C Äá»ŠNH: "")
    Placeholder = "Type here...",        -- Placeholder (Máº¶C Äá»ŠNH: "Enter text...")
    Callback = function(text)
        print("Username entered:", text)
        
        -- Xá»­ lÃ½ text
        if text ~= "" then
            print("Target set to:", text)
        end
    end
})

-- Thay Ä‘á»•i giÃ¡ trá»‹ Input tá»« code:
UsernameInput.SetValue("NewUsername")

-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
-- 4.5: DROPDOWN (MENU THáº¢ XUá»NG)
-- â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

local WeaponDropdown = SeaMinhUI:AddDropdown(CombatTab, {
    Title = "Select Weapon",             -- TiÃªu Ä‘á» (Báº®T BUá»˜C)
    Description = "Choose your weapon",  -- MÃ´ táº£ (TÃ™Y CHá»ŒN)
    Values = {"Sword", "Gun", "Bow", "Magic Staff"}, -- Danh sÃ¡ch giÃ¡ trá»‹ (Báº®T BUá»˜C)
    Default = "Sword",                   -- GiÃ¡ trá»‹ máº·c Ä‘á»‹nh (Máº¶C Äá»ŠNH: giÃ¡ trá»‹ Ä‘áº§u tiÃªn)
    Callback = function(value)
        print("Weapon selected:", value)
        
        -- Equip weapon
        if value == "Sword" then
            print("Equipped Sword!")
        elseif value == "Gun" then
            print("Equipped Gun!")
        end
    end
})

-- Thay Ä‘á»•i giÃ¡ trá»‹ Dropdown tá»« code:
WeaponDropdown.SetValue("Gun")

-- ============================================================
-- VÃ Dá»¤ HOÃ€N CHá»ˆNH: SCRIPT AUTO FARM
-- ============================================================

-- Load UI
local SeaMinhUI = loadstring(game:HttpGet("url"))()

-- Táº¡o Window
local Window = SeaMinhUI:CreateWindow({
    Title = "ğŸŒŠ Sea Minh Farm Hub",
    SubTitle = "Premium Edition v2.5",
    Size = UDim2.fromOffset(600, 480),
    TabWidth = 160,
    Theme = "Aqua",
    Acrylic = true,
    MinimizeKey = Enum.KeyCode.RightControl,
    Icon = "crown",
    IconSize = 26
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- TAB 1: FARMING
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local FarmTab = SeaMinhUI:AddTab(Window, {
    Title = "âš¡ Farming",
    Icon = "zap"
})

-- Variables
local autoFarmEnabled = false
local farmSpeed = 16
local selectedMob = "Bandit"

-- Auto Farm Toggle
local FarmToggle = SeaMinhUI:AddToggle(FarmTab, {
    Title = "Auto Farm",
    Description = "Automatically farm selected mob",
    Default = false,
    Callback = function(value)
        autoFarmEnabled = value
        
        if value then
            print("âœ… Auto Farm Started!")
            
            -- Auto Farm Loop
            spawn(function()
                while autoFarmEnabled do
                    -- Farming logic here
                    print("Farming", selectedMob, "...")
                    wait(1)
                end
            end)
        else
            print("âŒ Auto Farm Stopped!")
        end
    end
})

-- Select Mob
SeaMinhUI:AddDropdown(FarmTab, {
    Title = "Select Mob",
    Description = "Choose mob to farm",
    Values = {"Bandit", "Boss", "Elite Guard", "Dragon"},
    Default = "Bandit",
    Callback = function(value)
        selectedMob = value
        print("ğŸ¯ Target:", value)
    end
})

-- Farm Speed
SeaMinhUI:AddSlider(FarmTab, {
    Title = "Farm Speed",
    Description = "How fast to farm",
    Min = 1,
    Max = 10,
    Default = 5,
    Rounding = 0,
    Callback = function(value)
        farmSpeed = value
        print("âš¡ Speed:", value)
    end
})

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
